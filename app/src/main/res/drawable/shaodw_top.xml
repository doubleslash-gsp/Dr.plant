<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <item>
        <shape>
            <padding android:top="2dp"  android:bottom="2dp"/>
            <stroke android:color="#02000000" android:width="10dp"  />
        </shape>
    </item>
    <item>
        <shape>
            <padding android:top="2dp" />
            <stroke android:color="#05000000" android:width="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <padding android:top="2dp" />
            <stroke android:color="#10000000" android:width="1dp" />
        </shape>
    </item>
    <item>
        <shape>
            <padding android:top="2dp" />
            <stroke android:color="#15000000" android:width="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <padding android:top="2dp" />
            <stroke android:color="#20000000" android:width="5dp" />
        </shape>
    </item>
    <!-- Background -->
    <item>
        <shape>
            <solid android:color="#FFF" />
        </shape>
    </item>
</layer-list>